{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
	    "browser": true,
	    "node": true
	},
	"parser": "babel-eslint",
    "arrow-body-style": ["error", "always"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "class-methods-use-this": ["error", { "exceptMethods": ["responseGoogle", "errorGoogle", 
        "logMeOut", "componentWillMount", "componentWillUnmount", "render"] }]
    }
}